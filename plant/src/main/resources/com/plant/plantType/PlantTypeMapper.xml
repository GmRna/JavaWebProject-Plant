<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plant.plantType.PlantTypeMapper">
	
	<select id="plantStypeF" parameterType="String" resultType="com.plant.plantType.PlantTypeVO">
		SELECT DISTINCT svcCodeNm 
		FROM plantType 
		WHERE prdlstCtgCode LIKE '${stype}%'
	</select>
	<!-- AND svcCodeNm NOT LIKE '%>%' 뻈음 -->
	
	<select id="plantStypeS" parameterType="String" resultType="com.plant.plantType.PlantTypeVO">
		SELECT DISTINCT cntntsSj 
		FROM plant.plantType 
		WHERE svcCodeNm LIKE '%${stype}%'
	</select>
	
</mapper>